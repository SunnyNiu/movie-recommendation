{
  "name": "react-form-demo",
  "version": "0.2.0",
  "description": "Form handling using React",
  "main": "server/server.js",
  "scripts": {
    "cypress:version": "cypress version",
    "cypress:verify": "cypress verify",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:record": "cypress run --record",
    "cypress:run:chrome": "cypress run --browser chrome",
    "cypress:run:headed": "cypress run --headed",
    "client": "webpack-dev-server --hot --inline",
    "dev": "nodemon server & webpack --watch",
    "server": "node server/index.js",
    "start": "webpack && node server/index.js",
    "db:reset": "run-s db:delete db:migrate db:seed",
    "db:migrate": "npm run knex migrate:latest",
    "db:seed": "npm run knex seed:run",
    "test": "jest",
    "postinstall": "webpack && knex migrate:latest",
    "e2e": "cypress open",
    "ci:dev":"start-server-and-test start :3000 test",
    "ci":"start-server-and-test start :3000 test:headless",
    "lint": "eslint --fix cypress/integration/*.js cypress/support/*.js cypress/plugins/*.js"
  },
  "author": "Sunny",
  "license": "ISC",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "jest": {
    "setupFiles": [
      "./tests/setup.js"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.0.0",
    "@cypress/webpack-preprocessor": "^4.1.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.0.5",
    "cypress": "^3.8.1",
    "cypress-axe": "0.5.3",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "jest": "^24.9.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.1",
    "serverless-webpack": "^5.3.1",
    "supertest": "^4.0.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1",
    "start-server-and-test": "^1.10.6",
    "eslint": "6.8.0",
    "eslint-plugin-cypress-dev": "3.0.0",
    "eslint-plugin-mocha": "5.3.0"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "css-loader": "^3.2.0",
    "express": "^4.17.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest-styled-components": "^6.3.4",
    "knex": "^0.20.1",
    "nock": "^11.7.0",
    "node-sass": "^4.13.0",
    "nodemon": "^1.19.4",
    "react-redux": "^7.1.3",
    "redux": "^4.0.4",
    "redux-mock-store": "^1.5.4",
    "redux-thunk": "^2.3.0",
    "sqlite3": "^4.1.0",
    "start-server-and-test": "^1.10.6",
    "style-loader": "^1.0.0",
    "styled-components": "^4.4.1",
    "superagent": "^5.1.0",
    "supertest": "^4.0.2",
    "todomvc-common": "^1.0.5"
  },
  "repository": "https://github.com/SunnyNiu/movie-recommendation.git"
}
