{
  "name": "movie-recommendation",
  "version": "0.2.0",
  "description": "Recommend movies base on people choice",
  "main": "server/server.js",
  "scripts": {
    "cypress:version": "cypress version",
    "cypress:verify": "cypress verify",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:record": "cypress run --record",
    "cypress:run:chrome": "cypress run --browser chrome",
    "cypress:run:headed": "cypress run --headed",
    "client": "webpack-dev-server --hot --inline",
    "dev": "nodemon server & webpack --watch",
    "server": "node server/index.js",
    "start": "webpack && node server/index.js",
    "db:reset": "run-s db:delete db:migrate db:seed",
    "db:migrate": "npm run knex migrate:latest",
    "db:seed": "npm run knex seed:run",
    "test": "jest",
    "postinstall": "webpack && knex migrate:latest",
    "e2e": "start-server-and-test start :3000 'cypress run'",
    "ci": "start-server-and-test start :3000 test:headless",
    "prettify": "prettier --config ./.prettierrc --write 'src/**/*.js'",
    "lint": "eslint src"
  },
  "author": "Sunny",
  "license": "MIT",
  "jest": {
    "setupFiles": [
      "./tests/setup.js"
    ],
    "reporters": [
      "default",
      [
        "./node_modules/jest-html-reporter",
        {
          "pageTitle": "Test Report"
        }
      ]
    ],
    "verbose": true
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/node": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.0.0",
    "@cypress/webpack-preprocessor": "^4.1.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-styled-components": "^1.10.6",
    "cypress": "^3.8.1",
    "cypress-axe": "0.5.3",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-import-resolver-webpack": "^0.12.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "jest": "^24.9.0",
    "jest-junit": "^10.0.0",
    "prettier": "^2.0.2",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.0.1",
    "react-test-renderer": "^16.13.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-mock-store": "^1.5.4",
    "serverless-webpack": "^5.3.1",
    "start-server-and-test": "^1.10.6",
    "supertest": "^4.0.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1"
  },
  "dependencies": {
    "clean-webpack-plugin": "^3.0.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "html-webpack-plugin": "^3.2.0",
    "jest-html-reporter": "^2.8.0",
    "jest-styled-components": "^6.3.4",
    "knex": "^0.20.1",
    "nock": "^11.7.2",
    "path": "^0.12.7",
    "react-redux": "^7.1.3",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "sqlite3": "^4.1.0",
    "styled-components": "^4.4.1",
    "superagent": "^5.1.0"
  },
  "repository": "https://github.com/SunnyNiu/movie-recommendation.git"
}
